<?xml version="1.0" encoding="UTF-8"?>
<proxy name="JMSProxy" startOnLoad="true" transports="jms" xmlns="http://ws.apache.org/ns/synapse">
    <target>
        <inSequence>
            <log level="full">
                <property name="Log" value="Picking from ActiveMQ Queue"/>
            </log>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </target>
    <parameter name="transport.jms.ConnectionFactory">myQueueListener</parameter>
</proxy>
